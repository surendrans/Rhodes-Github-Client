<div class="pageTitle">
  <h1>My Repos</h1>
</div>

<div class="toolbar">
  <div class="leftItem backButton">
    <a class="cancel" href="<%= url_for :controller => :User, :action => :listing %>">Back</a>
  </div>
</div>
<div class='content'>
  <ul>
  <% get_res["repositories"].each do | repo | %>
  <li>
    <a href="<%= url_for :controller => :RepoInformation, :action => :index, :name => repo["name"] %>"><%= repo["name"] %> </a>
  </li>
    <% end%>
</ul>
</div>